  AgendaId NVARCHAR(30) NULL,
  Agenda NVARCHAR(50) NULL,
  StartDate DATE NULL,
  EndDate DATE NULL,
  DueDate DATE NULL,
  DaysPlanned INTEGER NULL,
  IsObsolete BOOLEAN NULL,

<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="4.0" xmlns:edmx="http://docs.oasis-open.org/odata/ns/edmx">
  <edmx:Reference Uri="https://sap.github.io/odata-vocabularies/vocabularies/Common.xml">
    <edmx:Include Alias="Common" Namespace="com.sap.vocabularies.Common.v1"/>
  </edmx:Reference>
  <edmx:Reference Uri="https://oasis-tcs.github.io/odata-vocabularies/vocabularies/Org.OData.Core.V1.xml">
    <edmx:Include Alias="Core" Namespace="Org.OData.Core.V1"/>
  </edmx:Reference>
  <edmx:DataServices>
    <Schema Namespace="OrderManagementService" xmlns="http://docs.oasis-open.org/odata/ns/edm">
      <Annotation Term="Core.Links">
        <Collection>
          <Record>
            <PropertyValue Property="rel" String="author"/>
            <PropertyValue Property="href" String="https://cap.cloud.sap"/>
          </Record>
        </Collection>
      </Annotation>
      <EntityContainer Name="EntityContainer">
        <EntitySet Name="Orders" EntityType="OrderManagementService.Orders">
          <NavigationPropertyBinding Path="items" Target="OrderItems"/>
        </EntitySet>
        <EntitySet Name="OrderItems" EntityType="OrderManagementService.OrderItems">
          <NavigationPropertyBinding Path="orderID" Target="Orders"/>
          <NavigationPropertyBinding Path="texts" Target="OrderItems_texts"/>
          <NavigationPropertyBinding Path="localized" Target="OrderItems_texts"/>
        </EntitySet>
        <EntitySet Name="OrderItems_texts" EntityType="OrderManagementService.OrderItems_texts"/>
      </EntityContainer>
      <EntityType Name="Orders">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <Property Name="customerID" Type="Edm.String"/>
        <Property Name="orderDate" Type="Edm.DateTimeOffset"/>
        <Property Name="TotalAmount" Type="Edm.Decimal" Precision="10" Scale="2"/>
        <NavigationProperty Name="items" Type="Collection(OrderManagementService.OrderItems)" Partner="orderID">
          <OnDelete Action="Cascade"/>
        </NavigationProperty>
      </EntityType>
      <EntityType Name="OrderItems">
        <Key>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <NavigationProperty Name="orderID" Type="OrderManagementService.Orders" Partner="items">
          <ReferentialConstraint Property="orderID_ID" ReferencedProperty="ID"/>
        </NavigationProperty>
        <Property Name="orderID_ID" Type="Edm.Guid"/>
        <Property Name="productName" Type="Edm.String"/>
        <Property Name="Quantity" Type="Edm.Int32"/>
        <Property Name="UnitPrice" Type="Edm.Decimal" Precision="10" Scale="2"/>
        <Property Name="TotalPrice" Type="Edm.Decimal" Precision="10" Scale="2"/>
        <NavigationProperty Name="texts" Type="Collection(OrderManagementService.OrderItems_texts)">
          <OnDelete Action="Cascade"/>
        </NavigationProperty>
        <NavigationProperty Name="localized" Type="OrderManagementService.OrderItems_texts">
          <ReferentialConstraint Property="ID" ReferencedProperty="ID"/>
        </NavigationProperty>
      </EntityType>
      <EntityType Name="OrderItems_texts">
        <Key>
          <PropertyRef Name="locale"/>
          <PropertyRef Name="ID"/>
        </Key>
        <Property Name="locale" Type="Edm.String" MaxLength="14" Nullable="false"/>
        <Property Name="ID" Type="Edm.Guid" Nullable="false"/>
        <Property Name="productName" Type="Edm.String"/>
      </EntityType>
      <Annotations Target="OrderManagementService.Orders/ID">
        <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
      </Annotations>
      <Annotations Target="OrderManagementService.OrderItems/ID">
        <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
      </Annotations>
      <Annotations Target="OrderManagementService.OrderItems_texts/locale">
        <Annotation Term="Common.Label" String="{i18n>LanguageCode}"/>
      </Annotations>
      <Annotations Target="OrderManagementService.OrderItems_texts/ID">
        <Annotation Term="Core.ComputedDefaultValue" Bool="true"/>
      </Annotations>
    </Schema>
  </edmx:DataServices>
</edmx:Edmx>